{
  "version": 2,
  "builds": [
    {
      "src": "app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.ts"
    }
  ],
  "buildCommand": "prisma generate && tsc",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  }
} 